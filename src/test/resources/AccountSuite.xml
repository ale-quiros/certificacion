<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Account Test">
    <test name="Login Test - Chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="selenium.TestAccount"></class>
        </classes>
    </test>
    <test name="Search Test - FireFox">
        <parameter name="browser" value="firefox"></parameter>
        <parameter name="searchCriteria" value="iphone"></parameter>
        <parameter name="expectedResult" value="1"></parameter>
        <classes>
            <class name="selenium.TestSearch"></class>
        </classes>
    </test>
    <!--
    <test name="Login Test - IE">
        <parameter name="browser" value="ie"></parameter>
        <classes>
            <class name="selenium.TestAccount"></class>
        </classes>
    </test>
    -->
</suite>